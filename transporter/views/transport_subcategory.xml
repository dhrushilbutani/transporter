<odoo>
    <record id="transport_subcategory_tree" model="ir.ui.view">
        <field name="name">Transport Subcategory : Tree</field>
        <field name="model">transport.subcategory</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="name"/>
                <field name="category_id"/>
                <field name="min_weight"/>
                <field name="max_weight"/>
            </tree>
        </field>
    </record>
    <record id="transport_subcategory_action" model="ir.actions.act_window">
        <field name="name">Subcategory</field>
        <field name="res_model">transport.subcategory</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>